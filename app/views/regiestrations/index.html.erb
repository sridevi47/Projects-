<p id="notice"><%= notice %></p>

<h1>Regiestrations</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Cell</th>
      <th>Gender</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regiestrations.each do |regiestration| %>
      <tr>
        <td><%= regiestration.first_name %></td>
        <td><%= regiestration.last_name %></td>
        <td><%= regiestration.cell %></td>
        <td><%= regiestration.gender %></td>
        <td><%= regiestration.location %></td>
        <td><%= link_to 'Show', regiestration %></td>
        <td><%= link_to 'Edit', edit_regiestration_path(regiestration) %></td>
        <td><%= link_to 'Destroy', regiestration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regiestration', new_regiestration_path %>
